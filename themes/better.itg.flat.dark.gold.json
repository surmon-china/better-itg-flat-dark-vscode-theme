      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "background": "#f35656",
        "fontStyle": "",
        "foreground": "#F8F8F0"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "background": "#74f1c6",
        "foreground": "#F8F8F0"
      }
    },
    {
      "name": "JSON String",
