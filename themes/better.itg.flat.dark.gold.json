      "settings": {
        "foreground": "#f35656"
      }
    },
    {
      "name": "diff.inserted",
      "scope": "Markdown.inserted",
      "settings": {
        "foreground": "#a7e450"
      }
    },
    {
      "name": "diff.changed",
      "scope": "Markdown.changed",
      "settings": {
        "foreground": "#f0da5e"
      }
